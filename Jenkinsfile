pipeline{
  agent any{
    stages{
      stage('hello'){
        steps{
          echo 'hello world'
        }
      }
      stage('build'){
        steps{
          echo 'building the project'
        }
      }
      stage('test'){
        steps{
          echo 'Testing ...'
        }
      }
      stage('deploy'){
        steps{
          echo 'Deploying the app..'
        }
      }
    }
  }
}
  
